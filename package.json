{
  "name": "@oliver-schoendorn/react-date-time-range-picker",
  "version": "1.0.0",
  "description": "Date time range picker for React written in Typescript with internationalization in mind.",
  "keywords": [
    "react",
    "typescript",
    "date",
    "time",
    "range",
    "picker",
    "i18n",
    "internationalization"
  ],
  "repository": "github:oliver-schoendorn/react-date-time-range-picker",
  "files": [
    "dist/*",
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "main": "index.js",
  "types": "dist/esm/index.d.ts",
  "module": "dist/esm/index.js",
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "deep-equal": "^1.1.1",
    "immutability-helper": "^3.0.1",
    "shallowequal": "^1.1.0",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/deep-equal": "^1.0.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^11.15.7",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@types/react-intl": "^2.3.18",
    "@types/shallowequal": "^1.1.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "babel-preset-minify": "^0.5.1",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.6.0",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "react-intl": "^2.9.0",
    "resolve-url-loader": "^3.1.1",
    "rimraf": "^2.7.1",
    "sass-loader": "^7.3.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "tslint": "^5.20.1",
    "tslint-react": "^4.2.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "scripts": {
    "test": "jest --coverage",
    "start": "HMR=true webpack-dev-server --hot --colors --config webpack.examples.config.js",
    "clear": "rimraf ./dist/cjs/* && rimraf ./dist/esm/*",
    "build:types": "tsc",
    "build:styles": "cp -R ./src/Styles ./dist/esm",
    "build:bundle": "webpack --config webpack.bundle.config.js",
    "build:bundle:min": "webpack --config webpack.production.config.js --json > ./dist/cjs/stats.json",
    "build:analyze": "webpack-bundle-analyzer ./dist/cjs/stats.json -O -m static -r ./dist/cjs/stats.html",
    "build": "npm run clear && npm run build:types && npm run build:styles && npm run build:bundle && npm run build:bundle:min && npm run build:analyze",
    "prepublishOnly": "npm run test && npm run build"
  },
  "author": "Oliver Sch√∂ndorn <info@oswebstyle.de>",
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "testRegex": "(/src/.*\\.spec)\\.ts$",
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    },
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "./src/**/*",
      "!**/node_modules/**"
    ]
  }
}
