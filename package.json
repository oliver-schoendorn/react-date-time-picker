{
  "name": "@oliver-schoendorn/react-date-time-range-picker",
  "version": "1.1.0",
  "description": "Date time range picker for React written in Typescript with internationalization in mind.",
  "keywords": [
    "react",
    "typescript",
    "date",
    "time",
    "range",
    "picker",
    "i18n",
    "internationalization"
  ],
  "repository": "github:oliver-schoendorn/react-date-time-range-picker",
  "files": [
    "dist/*",
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "main": "index.js",
  "types": "dist/esm/index.d.ts",
  "module": "dist/esm/index.js",
  "dependencies": {
    "@babel/runtime": "^7.12.13",
    "deep-equal": "^2.0.5",
    "immutability-helper": "^3.1.1",
    "shallowequal": "^1.1.0",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-transform-modules-commonjs": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.16",
    "@types/deep-equal": "^1.0.1",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.26",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/shallowequal": "^1.1.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-preset-minify": "^0.5.1",
    "canvas": "^2.6.1",
    "css-loader": "^5.0.2",
    "cssnano": "^4.1.10",
    "fibers": "^4.0.3",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.6",
    "node-sass": "^5.0.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^4.2.0",
    "postcss-preset-env": "^6.7.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.13.0",
    "react-intl": "^5.12.2",
    "resolve-url-loader": "^3.1.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.1.1",
    "source-map-loader": "^1.1.3",
    "style-loader": "^2.0.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-react": "^5.0.0",
    "typescript": "^4.1.5",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  },
  "peerDependencies": {
    "react": "^16.13.1 || ^17.0.0"
  },
  "scripts": {
    "test": "jest --coverage",
    "start": "HMR=true webpack-dev-server --hot --colors --config webpack.examples.config.js",
    "clear": "rimraf ./dist/cjs/* && rimraf ./dist/esm/*",
    "build:types": "tsc",
    "build:styles": "cp -R ./src/Styles ./dist/esm",
    "build:bundle": "webpack --config webpack.bundle.config.js",
    "build:bundle:min": "webpack --config webpack.production.config.js --json > ./dist/cjs/stats.json",
    "build:analyze": "webpack-bundle-analyzer ./dist/cjs/stats.json -O -m static -r ./dist/cjs/stats.html",
    "build": "npm run clear && npm run build:types && npm run build:styles && npm run build:bundle && npm run build:bundle:min && npm run build:analyze",
    "prepublishOnly": "npm run test && npm run build"
  },
  "author": "Oliver Sch√∂ndorn <info@oswebstyle.de>",
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "testRegex": "(/src/.*\\.spec)\\.ts$",
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    },
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "./src/**/*",
      "!**/node_modules/**"
    ]
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.3",
    "utf-8-validate": "^5.0.4"
  }
}
